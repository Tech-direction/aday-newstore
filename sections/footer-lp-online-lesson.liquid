<link
  {% # スタイルシートを読み込むためのリンクタグ %}
  rel="stylesheet"
  {% # 読み込むスタイルシートのURLを指定 %}
  href="{{ 'section-footer-lp-online-lesson.css' | asset_url }}"
  {% # プリントメディア用にスタイルシートを読み込む %}
  media="print"
  {% # 読み込みが完了したらメディアを全てに変更 %}
  onload="this.media='all'"
>

